{
  "name": "startuplagosbackend",
  "version": "1.0.0",
  "description": "The backend repo built in nodejs for startup lagos application",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --exec babel-node server",
    "prod:build": "npm run clean && npm run build && npm run start",
    "start": "node dist/index.js",
    "build": "babel server --out-dir dist",
    "clean": "rm -rf ./build "
  },
  "author": "ekoinnovationhub",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.9.2",
    "nodemon": "^2.0.4",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5"
  }
}
